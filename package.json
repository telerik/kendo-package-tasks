{
  "name": "@telerik/kendo-package-tasks",
  "description": "An utility package exporting gulp tasks for Kendo UI NPM packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-package-tasks.git"
  },
  "main": "gulp-tasks.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "Kendo UI"
  ],
  "dependencies": {
    "@telerik/eslint-config": "^1.1.0",
    "@telerik/kendo-common-tasks": "^3.11.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-umd": "~6",
    "babel-preset-es2015": "~6",
    "babel-preset-stage-1": "~6",
    "babel-traverse": "6.3.19",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "gulp-buble": "^0.9.0",
    "gulp-jasmine": "^2.2.1",
    "jasmine-reporters": "^2.1.1",
    "jasmine-spec-reporter": "^2.4.0",
    "jquery": "^2.2.1",
    "json-loader": "^0.5.4",
    "merge2": "1.0.3",
    "rimraf": "^2.5.4",
    "rollup-plugin-buble": "0.14.0",
    "rollup-stream": "1.14.0",
    "source-map-loader": "^0.1.5",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "yargs": "^3.30.0"
  },
  "author": "Telerik",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "1.10.3",
    "eslint-plugin-react": "^3.0.0",
    "ghooks": "^1.0.3",
    "json-loader": "^0.5.4",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  }
}
